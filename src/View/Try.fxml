<?xml version="1.0" encoding="UTF-8"?>

<?import View.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<Pane maxHeight="800.0" maxWidth="800.0" minHeight="800.0" minWidth="800.0" onKeyPressed="#keyPressed" stylesheets="@ViewStyle.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MyViewController">
   <children>
      <Pane layoutX="150.0" layoutY="168.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="500.0" prefWidth="500.0">
          <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ebf5ff" height="500.0" layoutX="-2.0" stroke="BLACK" strokeType="INSIDE" width="500.0" />
              <MazeDisplayer fx:id="mazeDisplayer" height="500.0" layoutX="-2.0" nodeOrientation="INHERIT" width="500.0" />
            <ImageView fx:id="finishImage" fitHeight="423.0" fitWidth="420.0" layoutX="40.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true" visible="false">
               <image>
                  <Image url="@Images/finish-White.jpg" />
               </image>
            </ImageView>
          </children>
      </Pane>
      <GridPane layoutX="280.0" layoutY="50.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="141.0" minWidth="10.0" prefWidth="134.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="154.0" minWidth="10.0" prefWidth="66.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label prefHeight="18.0" prefWidth="140.0" text="Maze Rows:" />
            <Label prefHeight="18.0" prefWidth="140.0" text="Maze Columns:" GridPane.rowIndex="1" />
            <TextField fx:id="textField_mazeColumns" alignment="CENTER" text="50" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button mnemonicParsing="false" onAction="#GenerateMaze" prefHeight="26.0" prefWidth="265.0" text="Generate" GridPane.rowIndex="2" />
            <Button mnemonicParsing="false" onAction="#SolveMaze" prefHeight="26.0" prefWidth="104.0" text="Solve" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="textField_mazeRows" alignment="CENTER" text="50" GridPane.columnIndex="1" />
         </children>
      </GridPane>

       <VBox BorderPane.alignment="CENTER">
           <children>
               <MenuBar minWidth="800.0">
                   <menus>
                       <Menu mnemonicParsing="false" text="File">
                           <items>
                               <MenuItem mnemonicParsing="false" onAction="#GenerateMaze" text="New    Ctrl+F1" />
                               <MenuItem mnemonicParsing="false" text="Save   Ctrl+F2" />
                               <MenuItem mnemonicParsing="false" text="Load   Ctrl+F3" />
                               <MenuItem mnemonicParsing="false" onAction="#Exit" text="Exit" />
                           </items>
                       </Menu>
                       <Menu mnemonicParsing="false" text="Options">
                           <items>
                               <MenuItem mnemonicParsing="false" text="Properties" />
                           </items>
                       </Menu>
                       <Menu mnemonicParsing="false" text="Help">
                       </Menu>
                       <Menu mnemonicParsing="false" text="About">
                       </Menu>
                   </menus>
               </MenuBar>
           </children>
       </VBox>
      <ImageView fitHeight="126.0" fitWidth="77.0" layoutX="685.0" layoutY="38.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/Rick.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="126.0" fitWidth="125.0" layoutX="18.0" layoutY="39.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/Morty.png" />
         </image>
      </ImageView>
      <Label fx:id="labelCatch" alignment="CENTER" contentDisplay="CENTER" layoutX="150.0" layoutY="706.0" prefHeight="23.0" prefWidth="500.0">
         <font>
            <Font name="Yellowtail" size="25.0" />
         </font>
      </Label>
      <Button layoutX="310.0" layoutY="753.0" mnemonicParsing="false" onAction="#catchPhrase" prefHeight="25.0" prefWidth="188.0" text="Catch Phrase " />
      <ImageView fitHeight="152.0" fitWidth="134.0" layoutX="42.0" layoutY="281.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/bird.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="152.0" fitWidth="122.0" layoutX="669.0" layoutY="281.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/mr-poopy-buttholee.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="174.0" fitWidth="165.0" layoutX="649.0" layoutY="554.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/Mr_Meeseeks.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="193.0" fitWidth="101.0" layoutX="24.0" layoutY="544.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/ScaryTerrytransparent.gif" />
         </image>
      </ImageView>
      <Button layoutX="49.0" layoutY="218.0" mnemonicParsing="false" onAction="#finishMaze" text="bbb" />

   </children>
</Pane>
